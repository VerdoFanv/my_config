{
  "name": "myapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules && yarn install",
    "clean:win": "rd -r node_modules; yarn install",
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:build:apk": "ENVFILE=.env && cd android  && ./gradlew clean && ./gradlew app:assembleRelease && cd ..",
    "android:build:aab": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "ios": "react-native run-ios",
    "ios:16": "react-native run-ios --simulator='iPhone 14 Pro'",
    "pod:i": "cd ios && pod install && cd ..",
    "pod:i:m1": "cd ios && arch -x86_64 pod install && cd ..",
    "ios:11": "react-native run-ios --simulator='iPhone 11'",
    "start": "react-native start",
    "android:codepush:stag": "appcenter codepush release-react -a myapp/rn-android -d Staging",
    "android:codepush:prod": "appcenter codepush release-react -a myapp/rn-android -d Production",
    "test": "jest",
    "lint": "eslint ./src"
  },
  "dependencies": {
